matches the snapshot:
  1: |
    apiVersion: cascading.securecodebox.io/v1
    kind: CascadingRule
    metadata:
      labels:
        securecodebox.io/intensive: high
        securecodebox.io/invasive: invasive
        securecodebox.io/type: bruteforce
      name: ncrack-ftp
    spec:
      matches:
        anyOf:
          - attributes:
              port: 21
              state: open
            category: Open Port
          - attributes:
              service: ftp
              state: open
            category: Open Port
      scanSpec:
        parameters:
          - -v
          - -d10
          - -U
          - /ncrack/users.txt
          - -P
          - /ncrack/passwords.txt
          - -p
          - ftp:{{attributes.port}}
          - '{{$.hostOrIP}}'
        scanType: ncrack
